<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="mysqlgenerator" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 去除自动生成的注释 -->
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/wbsp?useUnicode=true&amp;characterEncoding=UTF-8"
                        userId="root"
                        password="huyang"/>
        <javaModelGenerator targetPackage="org.sfg.wbsp.domain" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="mapping" targetProject="src/main/resources"/>
        <javaClientGenerator type="XMLMAPPER" targetPackage="org.sfg.wbsp.dao" targetProject="src/main/java"/>
        <table schema="" tableName="product_category" enableCountByExample="false" enableSelectByExample="false" enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
        <table schema="" tableName="product_attribute" enableCountByExample="false" enableSelectByExample="false" enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"/>
    </context>
</generatorConfiguration>